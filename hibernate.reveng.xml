<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="comment" catalog="news">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="history" catalog="news">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="news" catalog="news">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="newstype" catalog="news">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="save" catalog="news">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
<table name="user" catalog="news">
	<primary-key>
		<generator class="increment"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>